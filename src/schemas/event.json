{
  "Main": {
    "uid": {
      "type": "UID",
      "config": {
        "label": "slug",
        "placeholder": "Unique URL of this event page. (ie. hackathon ; which becomes codethechangeyyc.ca/hackathon"
      }
    },
    "title": {
      "type": "StructuredText",
      "config": {
        "single": "heading2",
        "label": "Event Title",
        "placeholder": "[Series: ] Event Title"
      }
    },
    "date": {
      "type": "Timestamp",
      "config": {
        "label": "Event Date",
        "placeholder": "Date & Time of the event"
      }
    },
    "location": {
      "type": "StructuredText",
      "config": {
        "multi": "heading4",
        "label": "Event Location",
        "placeholder": "eg. Hosted At:"
      }
    },
    "details": {
      "type": "StructuredText",
      "config": {
        "multi": "paragraph, image, embed",
        "label": "Event Details",
        "placeholder": "Details/Content about the event"
      }
    },
    "signup_link": {
      "type": "Link",
      "config": {
        "label": "Signup Link",
        "placeholder": "(Optional) Link for users to sign-up",
        "allowTargetBlank": true
      }
    },
    "poster": {
      "type": "Image",
      "config": {
        "constraint": {},
        "thumbnails": [],
        "label": "Event PosterImage"
      }
    },
    "embed_link": {
      "type": "Embed",
      "config": {
        "label": "Embed Link",
        "placeholder": "(Optional) For third-party embed stuff. (ie. photo albums etc.)"
      }
    }
  }
}
